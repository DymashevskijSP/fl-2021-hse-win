{
	"scopeName": "source.ebnf",
	"patterns": [{"include" : "#operators"},{ "include": "#rulename" }, { "include": "#plaintext" }],
	"repository": {	
	  
	  "plaintext": {	
		"match": "\\'[^\\']*\\'",
		"name": "string.double.quoted.ebnf"
	  },
	  "rulename":{
		  "name": "keyword.control.ebnf",
		  "match": "([^\\,(/\\*)(\\*/)\\[\\]\\(\\)\\{\\}\\=\\|\\'\\#]|\\\\,|\\\\#|\\\\[|\\\\]|\\\\(|\\\\)|\\\\{|\\\\}|(\\\\=)|(\\|)|\\\\')+"
	  }
	,
	  "operators": {
		"match": "\\||,|\\[|\\]|\\(|\\)|\\{|\\}|\\=",
		"name": "constant.character.ebnf"	  
	  }
				

	}
  }